{
  "name": "angular-demo",
  "version": "0.0.0",
  "description": "",
  "main": "js/bundle.js",
  "scripts": {
    "start": "http-server -a localhost -p 8080",
    "watch-js": "watchify js/main.js -o js/build.js --debug --verbose",
    "build-js": "browserify js/main.js > js/build.js",
    "watch-less": "watch-lessc -i css/main.less -o css/main.css",
    "watch": "npm run watch-less & npm run watch-js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ascott1/angular-demo.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ascott1/angular-demo/issues"
  },
  "homepage": "https://github.com/ascott1/angular-demo",
  "dependencies": {
    "angular": "~1.3.0",
    "angular-ui-router": "~0.2.11",
    "normalize.css": "^3.0.2"
  },
  "devDependencies": {
    "http-server": "^0.6.1",
    "browserify": "^6.1.0",
    "watch-lessc": "0.0.2",
    "watchify": "^2.0.0"
  }
}
